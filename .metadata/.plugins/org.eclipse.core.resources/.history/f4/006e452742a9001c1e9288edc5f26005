package com.tys.assignment;

public class vehicle {
	int speed;
	String gasType;
	int no_of_wheels;
	vehicle()
	{
		
	}
	vehicle(int speed,String gasType,int no_of_wheels)
	{
		this.speed=speed;
		this.gasType=gasType;
		this.no_of_wheels = no_of_wheels;
	}
	public void brake()
	{
		System.out.println("stop....");
	}

}
