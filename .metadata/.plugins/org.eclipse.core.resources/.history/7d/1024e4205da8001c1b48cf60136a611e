package com.tyss.corejava;
import java.util.Scanner;
public class Tables {
	public static void main()
	{
		int n,n1,i;
		System.out.print("enter the no. for which the tables has to be printed and also the bottom no.:");
		Scanner t=new Scanner(System.in);
		n=t.nextInt();
		n1=t.nextInt();
		System.out.println(n+"TABLE");
		for(i=1;i<n1;i++)
		{
			System.out.println(n+"*"+i+"="+n*i);
		}
	}

}
