package com.tys.project;

public class Phone {
        int price,ram;
        String brand;
        public void phone()
        {
        	System.out.println("i bought a phone");
        }
        public void call()
        {
        	System.out.println("calling....");
        }
        public void play()
        {
        	System.out.println("playing....");
        }
        public void radio()
        {
        	System.out.println("playing songs....");
        }
        public void message()
        {
        	System.out.println("helloooooo....");
        }
        
}
