package com.tyss.corejava;

public class Switch {
	public static void main(String[] args) {
		int x = 3;
		switch (x) {
		default:
			System.out.println("go home!!");
		case 1:
			System.out.println("go to goa");
			break;
		case 2:
			System.out.println("go to delhi");
			break;
		case 3:
			System.out.println("go to chiock manguluru");
			break;

		}
		x = 5;
		switch (x) {
		case 1:
			System.out.println("go to goa");
			break;
		case 2:
			System.out.println("go to delhi");
			break;
		case 3:
			System.out.println("go to chiock manguluru");
			break;
		default:
			System.out.println("go home!!");
			break;
		}
	}

}
